{
  "name": "open-desk",
  "version": "0.0.1",
  "description": "Init",
  "main": "build/app.bundle.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js  --recursive",
    "test-full": "npm run test && npm run lint && npm run flow",
    "test:watch": "npm run test -- --watch",
    "build": "NODE_ENV=production NODE_PATH=. node ./node_modules/webpack/bin/webpack.js",
    "build:watch": "npm run build -- --watch",
    "dev:client": "cross-env NODE_ENV=development NODE_PATH=. webpack-dev-server --watch-poll",
    "dev:server": "node server.js",
    "run-server": "supervisor -e node,js,json -w server.js,lib,model,routes,config app.js",
    "lint": "eslint ./",
    "flow": "flow check",
    "server:dev": "NODE_ENV=development NODE_PATH=. npm run run-server",
    "server": "NODE_ENV=production NODE_PATH=. npm run run-server",
    "postinstall": "npm run link-full-reinit",
    "init-links": "cd ./src/lib/dnd-desk && npm link && cd ../spec && npm link",
    "add-links": "npm link dnd-desk && npm link spec",
    "link-full-reinit": "npm run init-links && npm run add-links"
  },
  "pre-commit": [
    "test-full"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://Akols3@bitbucket.org/Akols3/open-desk.git"
  },
  "author": "Eddort",
  "license": "ISC",
  "homepage": "https://bitbucket.org/Akols3/open-desk#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "flow-bin": "^0.69.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.0.5",
    "node-sass": "^4.8.3",
    "node-static": "^0.7.10",
    "postcss-loader": "^2.1.3",
    "pre-commit": "^1.2.2",
    "precss": "^3.1.2",
    "react-hot-loader": "^4.0.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "supervisor": "^0.12.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.21.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0",
    "config": "^1.30.0",
    "cross-env": "^5.1.4",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "mongoose": "^5.0.14",
    "mongoose-class-wrapper": "^0.9.0",
    "mongoose-decorators": "^0.2.0",
    "morgan": "^1.9.0",
    "react": "^16.3.0",
    "react-beautiful-dnd": "^6.0.2",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.2.5",
    "url-loader": "^1.0.1"
  }
}
